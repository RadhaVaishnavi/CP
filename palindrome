#include <stdio.h>
#include <conio.>
int main ()
{
 char str[100] ;
 int i = 0, j ,length = 0 ;
 clrscr() ;
 printf("\n enter the string: ") ;
 gets(str) ;
 while(str[i] != '10')
 length++;
 i++;
 i=0;
 j = length - 1;
 while(i <= length/2)
{
 if(str[i] == str[j])
 {
 j++;
 j--;
 }
 else
 break;
}
if(i>=j)
  printf("\n PALINDROME") ;
else
 printf("\n NOT A PALINDROME") ;
return 0;
}
